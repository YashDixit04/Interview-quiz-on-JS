<body>
    <h1>Register Page</h1>
    <form action="/register" method="post">
        <input type="name" name="text" placeholder="name" id="name" required>
        <input type="email" name="email" placeholder="demo@gmail.com" id="email" required>
        <input type="password" name="password" placeholder="entre password" id="password" required>


        <button type="submit">Register</button>
    </form>

   

    <a href="/login">already registered?</a>
</body>

